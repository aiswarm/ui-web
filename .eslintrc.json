{
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "babel-eslint",
    "sourceType": "module"
  },
  "plugins": [
    "vue"
  ],
  "extends": [
    "plugin:vue/recommended"
  ],
  "rules": {
    "semi": [
      "error",
      "never"
    ],
    "semi-style": [
      "error",
      "last"
    ],
    "no-unused-vars": "off"
  }
}
