{
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "@babel/eslint-parser",
    "requireConfigFile": false,
    "sourceType": "module",
    "ecmaVersion": 12
  },
  "plugins": [
    "vue"
  ],
  "extends": [
    "plugin:vue/recommended"
  ],
  "rules": {
    "multiline-comment-style": [
      "error",
      "starred-block"
    ],
    "no-lonely-if": "error",
    "comma-dangle": [
      "error",
      "never"
    ],
    "comma-spacing": [
      "error",
      {
        "before": false,
        "after": true
      }
    ],
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "never"
    ],
    "semi-style": [
      "error",
      "last"
    ],
    "curly": [
      "error",
      "all"
    ],
    "no-unused-vars": "off",
    "no-console": "off"
  }
}