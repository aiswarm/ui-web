{
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "@babel/eslint-parser",
    "requireConfigFile": false,
    "sourceType": "module",
    "ecmaVersion": 12
  },
  "plugins": [
    "vue"
  ],
  "extends": [
    "plugin:vue/recommended"
  ],
  "rules": {
    "semi": [
      "error",
      "never"
    ],
    "semi-style": [
      "error",
      "last"
    ],
    "no-unused-vars": "off",
    "no-console": "off"
  }
}